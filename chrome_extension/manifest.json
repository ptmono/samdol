{
    "name": "samdolc",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Bookmark a recruit.",

    "permissions": [
	"tabs",
	"contextMenus",
	"http://www.saramin.co.kr/*",
	"http://localhost:8559/*"

    ],

    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    },

    "background": {
	"scripts": ["contextmenu.js",
		    "tools/jquery-1.8.3.min.js",
		    "tools/jquery.bpopup-0.7.0.min.js"
		   ]
    }
}

